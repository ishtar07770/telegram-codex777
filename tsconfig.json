{
  "compilerOptions": {
    "target": "ES2021",
    "lib": ["es2021", "dom"],
    "module": "es2020",
    "moduleResolution": "bundler",
    "types": ["@cloudflare/workers-types"],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src/**/*"]
}
